public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		System.out.print("Enter the number whose factorial you want to find = ");
		int num = scanner.nextInt();
		
		int fact =1;
		for (int i=1; i<=num; i++) {
			fact = i* fact;
		}
		
		System.out.println(num+"! = "+fact);
		
		scanner.close();

}
